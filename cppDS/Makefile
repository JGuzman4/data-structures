# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release)
  cppDS_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := cppDS

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

cppDS:
ifneq (,$(cppDS_config))
	@echo "==== Building cppDS ($(cppDS_config)) ===="
	@${MAKE} --no-print-directory -C . -f cppDS.make config=$(cppDS_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f cppDS.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   cppDS"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"